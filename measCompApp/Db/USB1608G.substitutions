file "$(MEASCOMP)/db/measCompLongIn.template"
{
pattern
{ R,   MASK,   PORT,    ADDR}
{Li,   0xFF,  1608G_1,     0}
}

file "$(MEASCOMP)/db/measCompBinaryIn.template"
{
pattern
{ R,   MASK,   PORT,    ADDR}
{Bi1,  0x01   1608G_1,     0}
{Bi2,  0x02   1608G_1,     0}
{Bi3,  0x04   1608G_1,     0}
{Bi4,  0x08   1608G_1,     0}
{Bi5,  0x10   1608G_1,     0}
{Bi6,  0x20   1608G_1,     0}
{Bi7,  0x40   1608G_1,     0}
{Bi8,  0x80   1608G_1,     0}
}

