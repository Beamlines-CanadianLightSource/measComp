# Direction bits on binary I/O
#  VAL 0=input, 1=output
file "$(MEASCOMP)/db/measCompBinaryDir.template"
{
pattern
{ R,   MASK,  VAL,  PORT,    ADDR}
{Bd1,  0x01    0,  1608G_1,     0}
{Bd2,  0x02    0,  1608G_1,     0}
{Bd3,  0x04    0,  1608G_1,     0}
{Bd4,  0x08    0,  1608G_1,     0}
{Bd5,  0x10    1,  1608G_1,     0}
{Bd6,  0x20    1,  1608G_1,     0}
{Bd7,  0x40    1,  1608G_1,     0}
{Bd8,  0x80    1,  1608G_1,     0}
}

