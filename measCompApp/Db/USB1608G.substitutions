}

# Analog inputs
file "$(MEASCOMP)/db/measCompAnalogIn.template"
{
pattern
{ R,   PORT,   ADDR,  EGUL, LOPR, EGUF, HOPR,  RANGE,   SCAN,   PREC}
{Ai1, 1608G_1,    0,  -10., -10.,  10.,  10.,    3,  "1 second",   4}
{Ai2, 1608G_1,    1,  -10., -10.,  10.,  10.,    3,  "1 second",   4}
{Ai3, 1608G_1,    2,   -5.,  -5.,   5.,   5.,    2,  "1 second",   4}
{Ai4, 1608G_1,    3,   -5.,  -5.,   5.,   5.,    2,  "1 second",   4}
{Ai5, 1608G_1,    4,   -2.,  -2.,   2.,   2.,    1,  "1 second",   4}
{Ai6, 1608G_1,    5,   -2.,  -2.,   2.,   2.,    1,  "1 second",   4}
{Ai7, 1608G_1,    6,   -1.,  -1.,   1.,   1.,    0,  "1 second",   4}
{Ai8, 1608G_1,    7,   -1.,  -1.,   1.,   1.,    0,  "1 second",   4}
}

