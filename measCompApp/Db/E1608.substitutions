file "$(MEASCOMP)/db/measCompLongIn.template"
{
pattern
{R,    MASK,   PORT,    ADDR}
{Li,   0xFF,  E1608_1,     0}
}

file "$(MEASCOMP)/db/measCompBinaryIn.template"
{
pattern
{R,    MASK,    PORT,    ADDR}
{Bi1,  0x01,   E1608_1,     0}
{Bi2,  0x02,   E1608_1,     0}
{Bi3,  0x04,   E1608_1,     0}
{Bi4,  0x08,   E1608_1,     0}
{Bi5,  0x10,   E1608_1,     0}
{Bi6,  0x20,   E1608_1,     0}
{Bi7,  0x40,   E1608_1,     0}
{Bi8,  0x80,   E1608_1,     0}
}

