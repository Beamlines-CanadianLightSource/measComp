# Analog inputs
file "$(MEASCOMP)/db/measCompAnalogIn.template"
{
pattern
{  R,   PORT,   ADDR,  EGUL, LOPR, EGUF, HOPR,  RANGE,   SCAN,   PREC}
{ Ai1, E1608_1,    0,  -10., -10.,  10.,  10.,    3,  "1 second",   4}
{ Ai2, E1608_1,    1,  -10., -10.,  10.,  10.,    3,  "1 second",   4}
{ Ai3, E1608_1,    2,   -5.,  -5.,   5.,   5.,    2,  "1 second",   4}
{ Ai4, E1608_1,    3,   -5.,  -5.,   5.,   5.,    2,  "1 second",   4}
{ Ai5, E1608_1,    4,  -10., -10.,  10.,  10.,    3,  "1 second",   4}
{ Ai6, E1608_1,    5,  -10., -10.,  10.,  10.,    3,  "1 second",   4}
{ Ai7, E1608_1,    6,   -5.,  -5.,   5.,   5.,    2,  "1 second",   4}
{ Ai8, E1608_1,    7,   -5.,  -5.,   5.,   5.,    2,  "1 second",   4}
}

